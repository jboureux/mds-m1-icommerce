generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("CATEGORY_DATABASE_URL")
}

model Category {
  id        String   @id @default(cuid())
  name      String   @unique @db.VarChar(50)
  slug      String   @db.VarChar(50)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
